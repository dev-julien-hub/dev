</div>
<section class="p-2">
    <div class="pb-4">
        <h4>Tous mes projets</h4>
    </div>
    <div class="contanier">
        <div class="flex w-100 flex-wrap justify-content-md-start">
          <% for @i in @my_project %>
            <%= render 'layouts/project/my_extend_project' %>
          <% end %>
        </div>
    </div>
</section>
